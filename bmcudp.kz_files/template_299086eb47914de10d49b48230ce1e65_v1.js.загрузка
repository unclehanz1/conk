$(document).ready(function () {
    $("#slider1").tinycarousel({interval: true});
    $("#slider-partners").tinycarousel({interval: true});

    if ($('.rightSide').length) {
        $('.leftSide').css('width', '630px');
        $('.leftSide').css('float', 'left');
    }

    $("iframe[src^='https://www.youtube.com/embed/'], video").each(
        function () {
            $(this).css("max-width", $(this).width());
            $(this).attr("data-aspect", $(this).width() / $(this).height());
            $(this).width("100%");
        }
    );

    $("div.pricewrap").each(
        function () {
            div = $(this).parent();
            div.css("padding", "0px");
        }
    );

    $('p.sldr').each(function (i, el, a) {
        if ($('> img', el).length > 1) {
            id = 'myCarousel' + i;
            sldr = $('<div class="carousel slide center-block" data-ride="carousel" style="max-width: 800px"></div>');
            sldr.attr('id', id);
            indicators = $('<ol class="carousel-indicators"></ol>');
            inner = $('<div></div>').addClass('carousel-inner');

            $('> img', el).each(function (j, img, ar) {
                indicator = $('<li></li>');
                indicator.attr('data-target', '#' + id);
                indicator.attr('data-slide-to', j);
                indicators.append(indicator);

                item = $('<div class="item"></div>');
                item.append(img);
                inner.append(item);
            });

            $('> li', indicators).first().addClass('active');
            $('> div', inner).first().addClass('active');

            prev = $('<a class="carousel-control left" data-slide="prev"></a>');
            prev.attr('href', '#' + id);
            prev.append($('<i class="fa fa-chevron-left" aria-hidden="true"></i>'));

            next = $('<a class="carousel-control right" data-slide="next"></a>');
            next.attr('href', '#' + id);
            next.append($('<i class="fa fa-chevron-right" aria-hidden="true"></i>'));

            sldr.append(indicators);
            sldr.append(inner);
            sldr.append(prev);
            sldr.append(next);
            //sldr = $('<div></div>').append(sldr);
            //console.log(sldr.prop('outerHTML'));
            //console.log(sldr.html());
            $(el).replaceWith(sldr);
        }
    });

    $('.nav-pills a').on('shown.bs.tab', function (event) {
        resize_ppt();
    });
});

$(window).resize(function () {
    $("iframe[src^='https://www.youtube.com/embed/'], video").each(
        function () {
            $(this).height($(this).width() / $(this).attr("data-aspect"));
        }
    );

    $("iframe[src^='https://www.google.com/maps/d/embed'], iframe[src^='http://widgets.2gis.com/widget']").each(
        function () {
            docparent = $(this).parent();
            if (0.9 * docparent.width() < 834) {
                $(this).css("max-width", "100%");
                $(this).css("max-height", $(this).width() * 3.0 / 4.0 + 24);
            } else {
                $(this).css("max-width", "");
                $(this).css("max-height", "");
            }
        }
    );

    // pdf
    doc = $("iframe[src^='https://drive.google.com/file/d']");
    docparent = doc.parent();
    if (0.9 * docparent.width() < 834) {
        doc.width("100%"); //w / h = 0,7067137809187279
        doc.height((doc.width() - 34) / 0.7067 + 34);
    } else {
        doc.width("90%");
        doc.height(1132 + 34);
    }

    resize_ppt();
});

function resize_ppt() {
    $("iframe[src^='https://docs.google.com/presentation/']").each(
        function () {
            /*$(this).height($(this).width() / 1.617 + 29);*/
            $(this).height($(this).width() / 1.282);
            /*console.log("resize_ppt after: width="+$(this).width()+"; height="+$(this).height());*/
        }
    );
}
