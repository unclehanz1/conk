let lang_code = $('.site_language_code').attr('value');
$(function () {
    $('#sendthanks').click(function (e) {
        getMyForm();
        e.preventDefault();
    });
    $('body').on('submit', 'form[name=THANKS]', function (e) {
        $('.my__form_submit').prop('disabled', true);
        e.preventDefault();
        var dataForm = $(this).serialize() + '&web_form_submit=Y&lang=' + lang_code;
        sendMyForm(name, dataForm);
    });
});
$(document).bind('cbox_complete', function () {
    $.colorbox.resize();
});

function getMyForm(name, data) {
    $.ajax({
        url: '/' + lang_code + '/ajax/thanks_ajax.php',
        type: 'GET',
        data: data,
        success: function (data) {
            $.colorbox({
                html: data,
                maxHeight: "100%",
                maxWidth: "1170px;",
                width: "auto",
                height: "auto",
                fixed: false,
                reposition: true,
                inline: false
            });
        }
    });
}

function sendMyForm(name, data) {
    let error_text = {
        ru: "  ",
        kz: "  "
    }
    let success_text = {
        ru: "  ",
        kz: "  "
    }
    $.ajax({
        url: '/' + lang_code + '/ajax/thanks_ajax.php',
        type: 'POST',
        data: data,
    }).done(function (data) {
        $('.my__form_submit').prop('disabled', false);
        if (data == 'error') {
            $('.all__errors').html(" ");
            $('.all__errors').append("<p class='form-ansvalue'>" + error_text[lang_code] + "</p>");
        } else {
            $('.all__errors').html(" ");
            $.colorbox({
                html: '<div style="padding: 20px;"><div style="clear: both; min-width: 300px;" class="alert alert-success">' + success_text[lang_code] + '</div></div>',
                maxWidth: "1170px;",
                width: "auto",
                height: "auto",
                fixed: false,
                reposition: true,
                inline: false
            });
        }
    });
}
/* End */
